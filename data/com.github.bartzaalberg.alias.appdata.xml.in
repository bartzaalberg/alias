<component type="desktop">
  <id>com.github.bartzaalberg.alias</id>
  <metadata_license>CC0</metadata_license>
  <project_license>GPL-3.0+</project_license>
  <name>Alias</name>
  <summary>Simplify your commands</summary>
  <description>
    <p>A tool to manage your bash aliases</p>
    <p>Features:</p>
     <ul>
       <li>Manage your aliases</li>
       <li>Use handy shortcuts to navigate the app</li>
       <li>Choose between light and dark with dark mode</li>
    </ul>
  </description>
  <provides>
    <binary>com.github.bartzaalberg.alias</binary>
  </provides>
